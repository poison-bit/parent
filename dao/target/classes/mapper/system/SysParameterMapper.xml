<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.chw.dao.mapper.system.SysParameterDao">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.chw.dao.model.system.SysParameter">
        <id column="parameter_id" property="parameterId" />
        <result column="par_name" property="parName" />
        <result column="par_code" property="parCode" />
        <result column="par_type" property="parType" />
        <result column="par_value" property="parValue" />
        <result column="par_remark" property="parRemark" />
        <result column="state" property="state" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        parameter_id, par_name, par_code, par_type, par_value, par_remark, state
    </sql>

</mapper>
