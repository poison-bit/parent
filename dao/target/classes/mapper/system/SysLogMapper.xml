    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.chw.dao.model.system.SysLog">
        <id column="log_id" property="logId" />
        <result column="event_name" property="eventName" />
        <result column="log_type" property="logType" />
